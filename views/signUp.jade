extends layout

block content
  h1 Sign Up
  form(name='singUpForm' id='singUpForm' action='/users/signUp' method='post')
    p Email
      input(type='text' name='email' id='email')
    p Password
      input(type='text' name='password' id='password')
    p Password Confirmation
      input(type='text' name='passConf' id='passConf')
    button(type='Submit' class='submitButton') Submit
  p
    include errors
 
